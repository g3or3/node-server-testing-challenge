{
	"name": "node-server-testing",
	"version": "1.0.0",
	"description": "Node Server Testing Challenge",
	"scripts": {
		"start": "node index.js",
		"server": "nodemon index.js",
		"latest": "knex migrate:latest",
		"rollback": "knex migrate:rollback",
		"seed": "knex seed:run",
		"migrateh": "heroku run knex migrate:latest -a YOUR_HEROKU_APP_NAME_HERE",
		"rollbackh": "heroku run knex migrate:rollback -a YOUR_HEROKU_APP_NAME_HERE",
		"databaseh": "heroku pg:psql -a YOUR_HEROKU_APP_NAME_HERE",
		"seedh": "heroku run knex seed:run -a YOUR_HEROKU_APP_NAME_HERE",
		"test": "cross-env NODE_ENV=testing jest --watchAll --verbose --runInBand",
		"deploy": "git push heroku main"
	},
	"jest": {
		"testEnvironment": "node"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LambdaSchool/node-server-testing.git"
	},
	"engines": {
		"node": "14.16.0",
		"npm": "6.14.11"
	},
	"license": "ISC",
	"devDependencies": {
		"@types/jest": "^26.0.23",
		"cross-env": "^7.0.3",
		"eslint": "^7.13.0",
		"jest": "^26.6.3",
		"nodemon": "^2.0.7",
		"supertest": "^6.0.1"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"dotenv": "^9.0.2",
		"express": "^4.17.1",
		"helmet": "^4.6.0",
		"knex": "^0.95.6",
		"knex-cleaner": "^1.3.1",
		"pg": "^8.6.0"
	}
}
